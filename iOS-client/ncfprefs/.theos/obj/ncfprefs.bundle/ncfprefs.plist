<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>items</key>
	<array>
		<!-- General section -->
		<dict>
			<key>cell</key> <string>PSGroupCell</string>
			<key>footerText</key> <string>It is necessary to respring in order to apply settings reliably.</string>
		</dict>

		<dict>
			<key>action</key> <string>apply</string>
			<key>cell</key> <string>PSButtonCell</string>
			<key>label</key> <string>Respring to apply.</string>
		</dict>

		<dict>
			<key>cell</key> <string>PSLinkCell</string>
			<key>label</key> <string>Send notifications from</string><!-- PLS DONT EDIT -->
			<key>detail</key> <string>PBSendFromListController</string>
		</dict>

		<!-- Native section -->
		<dict>
			<key>cell</key> <string>PSGroupCell</string>
			<key>footerText</key> <string>Host IP is the IP address of the computer your notifications being sent to.</string>
		</dict>

		<dict>
			<key>cell</key> <string>PSSwitchCell</string>
			<key>default</key> <false/>
			<key>defaults</key> <string>org.h6nry.ncforward.prefs</string>
			<key>key</key> <string>enable</string>
			<key>label</key> <string>Enable NCForward</string>
			<key>PostNotification</key> <string>org.h6nry.ncforward.prefs/settingschanged</string>
		</dict>
		
		<dict>
			<key>cell</key> <string>PSEditTextCell</string>
			<key>default</key> <string>255.255.255.255</string>
			<key>defaults</key> <string>org.h6nry.ncforward.prefs</string>
			<key>isIP</key> <string>1</string>
			<key>key</key> <string>ip</string>
			<key>label</key> <string>Host IP</string>
			<key>PostNotification</key> <string>org.h6nry.ncforward.prefs/settingschanged</string>
		</dict>

		<!-- Pushbullet section -->
		<dict>
			<key>cell</key> <string>PSGroupCell</string>
			<key>footerText</key> <string>Pushbullet integration is optional.</string>
		</dict>

		<dict>
			<key>cell</key> <string>PSSwitchCell</string>
			<key>default</key> <false/>
			<key>defaults</key> <string>org.h6nry.ncforward.prefs</string>
			<key>key</key> <string>enablepushbullet</string>
			<key>label</key> <string>Pushbullet integration</string>
			<key>PostNotification</key> <string>org.h6nry.ncforward.prefs/settingschanged</string>
		</dict>

		<dict>
			<key>action</key> <string>pushbullet</string>
			<key>cell</key> <string>PSButtonCell</string>
			<key>label</key> <string>Activate Pushbullet</string>
			<key>PostNotification</key> <string>org.h6nry.ncforward.prefs/settingschanged</string>
		</dict>

		<dict>
			<key>cell</key> <string>PSLinkCell</string>
			<key>label</key> <string>Send notifications to</string><!-- PLS DONT EDIT -->
			<key>detail</key> <string>PBSendToListController</string>
		</dict>

		<!-- Additional section -->
		<dict>
			<key>cell</key> <string>PSGroupCell</string>
			<key>label</key> <string></string>
		</dict>
		
		<dict>
			<key>action</key> <string>twitter</string>
			<key>cell</key> <string>PSButtonCell</string>
			<key>label</key> <string>My Twitter @H6nry_</string>
		</dict>
		
		<dict>
			<key>action</key> <string>mail</string>
			<key>cell</key> <string>PSButtonCell</string>
			<key>label</key> <string>Write an e-mail to me</string>
		</dict>
		
		<dict>
			<key>action</key> <string>website</string>
			<key>cell</key> <string>PSButtonCell</string>
			<key>label</key> <string>My Website</string>
		</dict>
		
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>label</key> <string>License</string>
			<key>footerText</key>
			<string>Do what you want with it, just email me if you are going to publish parts of this. I also would like to be mentioned in the source code, if published in source code form, and credited, if published only in binary form. Due to the informality of this license, you are allowed to ignore this if you want to.</string>
		</dict>
		
		<dict>
			<key>cell</key>
			<string>PSGroupCell</string>
			<key>footerText</key>
			<string>Made with love in 2015 by H6nry (henry.anonym@gmail.com)</string>
		</dict>
	</array>
	<key>title</key>
	<string>NCForward</string>
</dict>
</plist>
